<h1><%= @user.name %></h1>
<% if @user.admin == true %>
  <p>ADMIN</p>
<% end %>
<p>Mood: <%= @user.mood %></p>
<p>Height: <%= @user.height %> inches</p>
<p>Number of Tickets: <%= @user.tickets %></p>

<p><%= link_to "See attractions", attractions_path %></p>

<p><%= link_to "Log Out", logout_path %></p>

<p><%= flash[:message] %></p>
